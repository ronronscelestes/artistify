<h1>All artists</h1>

<table>
<tbody>
<tr>
<td >picture</td>
<td >name</td>
<td >description</td>
<td >band</td>
<td >update</td>
<td >delete</td>
</tr>
{{#each artist}}
<tr>
<td><img src="{{this.picture}}" alt=""></td>
<td>{{this.name}}</td>
<td>{{this.description}}</td>
<td>{{this.isBand}}</td>
<td><a href="/dashboard/artist/update/{{this._id}}">update</a></td>
<td><a href="/dashboard/artist/delete/{{this._id}}">delete</a></td>
</tr>
{{/each}}
</tbody>
</table>